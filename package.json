{
  "name": "root",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@auto-it/npm": "^10.29.3",
    "@auto-it/released": "^10.29.3",
    "auto": "^10.29.3",
    "concurrently": "^6.2.0",
    "lerna": "^4.0.0",
    "prettier": "^2.3.2"
  },
  "scripts": {
    "build": "yarn workspace @kickstartds/storybook-addon-component-tokens build",
    "dev": "concurrently \"yarn workspace @kickstartds/storybook-addon-component-tokens dev\" \"yarn workspace examples storybook\"",
    "format": "prettier -w ."
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
