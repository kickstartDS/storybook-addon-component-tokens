{
  "name": "root",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@kickstartds/auto-config": "^1.1.2",
    "auto": "^10.46.0",
    "concurrently": "^8.2.0",
    "lerna": "^7.1.4",
    "prettier": "^3.0.0"
  },
  "scripts": {
    "build": "yarn workspace @kickstartds/storybook-addon-component-tokens build",
    "dev": "concurrently \"yarn workspace @kickstartds/storybook-addon-component-tokens dev\" \"yarn workspace examples storybook\" --raw",
    "format": "prettier -w ."
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
